<Project>

  <PropertyGroup>
    <LangVersion>preview</LangVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <NetCoreVersion>5.0.0-rc.2.20462.5</NetCoreVersion>
    <AspNetCoreVersion>5.0.0-rc.2.20460.25</AspNetCoreVersion>
    <ToolsetVersion>3.8.0-4.20462.1</ToolsetVersion>
    <AnalyzerVersion>5.0.0-rc2.20460.4</AnalyzerVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Net.Compilers.Toolset" Version="$(ToolsetVersion)" />
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="$(Analyzerversion)"
                      PrivateAssets="all" />
  </ItemGroup>

</Project>
