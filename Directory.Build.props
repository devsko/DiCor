<Project>

  <PropertyGroup>
    <LangVersion>preview</LangVersion>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <Nullable>enable</Nullable>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup>
    <NetCoreVersion>5.0.0-rc.2.20468.4</NetCoreVersion>
    <AspNetCoreVersion>5.0.0-rc-2-20469-5</AspNetCoreVersion>
    <ToolsetVersion>3.8.0-4.20469.1</ToolsetVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Net.Compilers.Toolset" Version="$(ToolsetVersion)" />
  </ItemGroup>

</Project>
